    "clsx": "^1.2.1",
    "cuid": "^2.1.8",
    "isbot": "^3.6.5",
    "prisma": "^4.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^4.2.1",
