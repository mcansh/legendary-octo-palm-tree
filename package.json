{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss --input ./styles/app.css --output ./app/styles/app.css --postcss --minify",
    "build:remix": "remix build",
    "build": "run-s \"build:*\"",
    "dev:css": "tailwindcss --input ./styles/app.css --output ./app/styles/app.css --postcss --watch",
    "dev:remix": "remix dev",
    "dev": "run-p \"dev:*\"",
    "start": "remix-serve ./build/index.js",
    "lint": "eslint --cache --fix --ignore-path .gitignore  .",
    "format": "prettier --write --ignore-path .gitignore  .",
    "db:migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@mcansh/remix-secure-headers": "^0.6.2",
    "@prisma/client": "^5.10.2",
    "@remix-run/node": "2.8.0",
    "@remix-run/react": "2.8.0",
    "@remix-run/serve": "2.8.0",
    "cloudinary": "^2.0.3",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^2.1.0",
    "cuid": "^2.1.8",
    "isbot": "^5.1.1",
    "prisma": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slugify": "^1.6.6",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@remix-run/dev": "2.8.0",
