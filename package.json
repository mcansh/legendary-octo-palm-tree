    "cuid": "^2.1.8",
    "isbot": "^3.6.5",
    "prisma": "^4.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.3.0",
    "remix-auth-form": "^1.2.0",
