    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-100vh-fix": "^1.0.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14"
  }
