    "@remix-run/react": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.3.0",
    "remix-auth-form": "^1.2.0",
    "slugify": "^1.6.5",
    "type-fest": "^3.2.0",
    "workers-qb": "^0.1.2",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
