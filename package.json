    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "better-sqlite3": "^8.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-prefer-let": "3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prisma": "^4.6.1",
    "remix-flat-routes": "^0.4.8",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "wrangler": "^2.2.1"
  }
